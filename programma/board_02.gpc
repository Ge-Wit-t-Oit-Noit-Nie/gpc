BEGIN_PROGRAMMA:
	ZET_POORT_AAN (POORT=0x00, HSIO=0x00);
	ZET_POORT_UIT (POORT=0x01, HSIO=0x00);
	
LOOP_PROGRAMMA:
	WACHTEN (delay=0x05DC);
	FLIP_POORT (POORT=0x01, HSIO=1);
	BEWAAR_STATUS;
	SPRING (LABEL=LOOP_PROGRAMMA);

PAUZE_PROGRAMMA:
	ZET_POORT_AAN (POORT=0x0F, HSIO=0x00);
	PAUZE;

EINDE_PROGRAMMA:
	BEWAAR_STATUS;
	STOPPEN;